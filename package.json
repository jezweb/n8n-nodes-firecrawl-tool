{
  "name": "n8n-nodes-firecrawl-tool",
  "version": "0.1.1",
  "description": "n8n node for Firecrawl v2 API - Web scraping, crawling, and data extraction tool for workflows and AI agents",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "firecrawl",
    "web-scraping",
    "crawling",
    "ai-tool",
    "data-extraction"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jezweb/n8n-nodes-firecrawl-tool",
  "author": {
    "name": "Jeremy Dawes",
    "email": "jeremy@jezweb.net",
    "url": "https://www.jezweb.com.au"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jezweb/n8n-nodes-firecrawl-tool.git"
  },
  "bugs": {
    "url": "https://github.com/jezweb/n8n-nodes-firecrawl-tool/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && cp -r icons dist/",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint nodes credentials --ext .ts",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/FirecrawlApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/FirecrawlTool/FirecrawlTool.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "n8n-workflow": "^1.25.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@mendable/firecrawl-js": "^3.2.1"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}